{
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": [
            0
        ],
        "indent": [
            2,
            "tab",
            {
                "SwitchCase": 1
            }
        ]
    },
    "env": {
        "node": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "arrowFunctions": true,
        "classes": true,
        "spread": true,
        "destructuring": true,
        "blockBindings": true,
        "experimentalObjectRestSpread": true
    },
    "plugins": []
}
